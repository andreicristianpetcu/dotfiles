  "repository": {
    "qq_angle_string_content": {
      "begin": "<",
      "end": ">",
      "patterns": [
        {
          "include": "#qq_angle_string_content"
        }
      ]
    },
    "qq_brace_string_content": {
      "begin": "{",
      "end": "}",
