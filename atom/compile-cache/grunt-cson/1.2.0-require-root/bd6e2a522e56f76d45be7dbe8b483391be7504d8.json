{
  ".text.xml": {
    "editor": {
      "commentStart": "<!-- ",
      "commentEnd": " -->",
      "foldEndPattern": "^\\s*(</[^>]+>|[/%]>|-->)\\s*$",
      "increaseIndentPattern": "^\\s*<(([^!/?]|%)(?!.+?([/%]>|</.+?>))|[%!]--\\s*$)",
      "decreaseIndentPattern": "^\\s*(</[^>]+>|-->|--%>)"
    }
  }
}
