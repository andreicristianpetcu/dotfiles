g/nrw/observ-varhash\n","maintainers":[{"name":"nrw","email":"nicholasredlin@gmail.com"}],"time":{"modified":"2015-05-28T20:33:47.946Z","created":"2014-06-05T21:58:19.010Z","0.0.0":"2014-06-05T21:58:19.010Z","0.0.1":"2014-06-06T00:45:09.374Z","0.0.2":"2014-06-06T02:20:59.428Z","0.1.0":"2014-06-07T13:35:34.761Z","0.1.1":"2014-06-07T14:04:20.183Z","0.1.2":"2014-06-09T02:46:21.219Z","0.1.3":"2014-07-06T07:02:22.878Z","0.1.4":"2014-07-18T07:40:06.035Z","0.2.0":"2014-07-18T09:12:28.154Z","0.3.0":"2014-10-15T17:10:03.107Z","1.0.0":"2014-12-01T21:13:09.112Z","1.0.1":"2014-12-15T14:29:29.586Z","1.0.2":"2014-12-16T07:37:04.758Z","1.0.3":"2014-12-23T04:58:35.454Z","1.0.4":"2014-12-23T05:15:35.533Z","1.0.5":"2015-03-18T21:56:45.370Z","1.0.6":"2015-05-28T20:33:47.946Z"},"homepage":"https://github.com/nrw/observ-varhash","keywords":[],"repository":{"type":"git","url":"git://github.com/nrw/observ-varhash.git"},"contributors":[{"name":"Nicholas Westlake"}],"author":{"name":"Nicholas Westlake","email":"nicholasredlin@gmail.com"},"bugs":{"url":"https://github.com/nrw/observ-varhash/issues","email":"nicholasredlin@gmail.com"},"readmeFilename":"Readme.md","_attachments":{},"_etag":"\"8BH33FNTYVT9FSFPN8NBQUEJG\""}